{
  "type": "module",
  "name": "@hazae41/glace",
  "version": "1.0.0",
  "description": "Create immutable websites",
  "homepage": "https://github.com/hazae41/glace",
  "repository": "github:hazae41/glace",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "prepare": "rm -rf ./out && tsc && tscousin",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "synchro": "deno -RW ./x.dsync.ts && deno install"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "devDependencies": {
    "@hazae41/phobos": "^2.0.10",
    "@hazae41/tscousin": "^1.0.20",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "immutable"
  ],
  "dependencies": {
    "@hazae41/mutex": "^3.0.0",
    "@hazae41/rewind": "^2.3.4",
    "esbuild": "^0.25.10",
    "happy-dom": "^19.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "bin": {
    "glace": "./out/bin.js"
  }
}
